source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
inhibit_all_warnings!

target "ArtSignProSdk" do
    use_frameworks!
    # 网络请求
    pod 'Alamofire'
    # 加载网络图片
    pod 'Kingfisher'
    # 扩展类
    pod 'EZSwiftExtensions'
    # json支持
    pod 'SwiftyJSON'
    # sha md5
    pod 'CryptoSwift'
    # ping++
    pod ''
end

target "ArtSignProSdkTests" do
    use_frameworks!
    # 测试框架
    pod 'Nimble'
end
