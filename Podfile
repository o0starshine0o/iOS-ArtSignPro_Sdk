source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
inhibit_all_warnings!
use_frameworks!

target "ArtSignProSdk" do
    # 网络请求
    pod 'Alamofire'
    # 加载网络图片
    pod 'Kingfisher'
    # 扩展类
    pod 'EZSwiftExtensions'
    # json支持
    pod 'SwiftyJSON'
    # sha md5
    pod 'CryptoSwift'
    # loading toast
    pod 'SwiftSpinner'
    # toast
    pod 'Toast-Swift'
    # checkbox
    pod 'M13Checkbox'
    # TextView 使用 placeHoldeer
    pod 'AutoLayoutTextViews'
end

target "ArtSignProSdkTests" do
    # 测试框架
    pod 'Nimble'
end
